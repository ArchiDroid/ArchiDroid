<device name="fmradio">
    <path name="on">
        <ctl name="DAC1L Mixer AIF1.1 Switch" val="1"/>
        <ctl name="DAC1L Mixer AIF1.1 Switch" val="1"/>
    </path>
    <path name="off">
        <ctl name="MIXINL Output Record Volume" val="0"/>
        <ctl name="MIXINR Output Record Volume" val="0"/>
